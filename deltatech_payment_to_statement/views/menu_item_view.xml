<odoo>
    <data>
		<record id="ax4b_conta_bancaria_form" model="ir.ui.view">
			<field name="name">Cadastro Bancário</field>
			<field name="model">account.bank.statement</field>
			<field name="inherit_id" ref="account.view_bank_statement_form"/>
			<field name="arch" type="xml">
				<field name="journal_id" position="attributes">
					<attribute name="domain">[()]</attribute>
				</field>
			</field>
		</record>


        <record id="cadastro_bancario_action_window" model="ir.actions.act_window" >
		    <field name="name">Cadastro Bancário</field>
			<field name="res_model">account.bank.statement</field>
			<field name="type">ir.actions.act_window</field>
			<field name="view_id" ref="account.view_bank_statement_form"/> 
         </record>
             
     	<menuitem id="cadastro_bancario_menu" name="Cadastro Bancário"    
			 parent="account.menu_finance_entries_actions"                 
			 action="cadastro_bancario_action_window"/> 
    </data>
</odoo>